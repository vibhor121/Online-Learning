{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "npm run install-all",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
